<!--page 相当于 UIViewController-->
<page title="GICXMLLayout" data-context="IndexPageViewModel">
    <!--list 相当于UITableView-->
    <list separator-inset = "0 15 0 15">
        <for data-path="listDatas">
            <list-item selection-style="2" item-height="45" event-item-select="onSelect:">
                <panel background-color="white">
                    <lable text="{{ name }}" margin="0 15 0.1 15"></lable>
                </panel>
            </list-item>
        </for>
    </list>
</page>

